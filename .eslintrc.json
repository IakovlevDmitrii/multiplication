{
  "extends": ["react-app", "react-app/jest", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "react-hooks/exhaustive-deps": "warn",
    "no-console": "warn",
    "prefer-const": "error",
    "indent": ["error", 2],
    "@typescript-eslint/quotes": ["error", "single"],
    "no-tabs": "error"
  }
}
